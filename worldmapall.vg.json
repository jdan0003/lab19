{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "title": "Best Restaurants Around the World",
    "projection": {"type": "equalEarth"},
    "layer": [
        {
          "data":{
            "url":"https://raw.githubusercontent.com/jdan0003/lab19/main/custom.geo.topo.json",
            "format": {"type":"topojson", "feature": "custom.geo"}
            },
          "mark": {"type":"geoshape", "fill": "lightgrey", "stroke": "white"}
        },
        {
          "data": {"url": "https://raw.githubusercontent.com/jdan0003/lab19/main/The%20Worlds%2050%20Best%20Restaurants%202018.csv"},
          "mark": {"type": "circle", "tooltip": {"content": "data"}},
          "encoding": {
              "longitude": {"field":"Longitude", "type": "quantitative"},
              "latitude": {"field": "Latitude", "type": "quantitative"},
              "size": {"value":10},
              "color": {"value":"red"
              }
          }
        },
        {
          "data": {"url": "https://raw.githubusercontent.com/jdan0003/lab19/dd22e27311bb2d2b9b021818b4c710b121be5645/50%20best%20restaurants%20database.csv"},
          "mark": {"type": "circle", "tooltip": {"content": "data"}},
          "encoding": {
              "longitude": {"field":"Longitude", "type": "quantitative"},
              "latitude": {"field": "Latitude", "type": "quantitative"},
              "size": {"value":10},
              "color": {"value":"blue"}
          }
        },
        {
          "data": {"url": "https://raw.githubusercontent.com/jdan0003/lab19/main/one-star-michelin-restaurants.csv"},
          "mark": {"type": "circle", "tooltip": {"content":"data"}},
          "encoding": {
            "longitude": {"field":"longitude", "type":"quantitative"},
            "latitude": {"field":"latitude","type":"quantitative"},
            "size": {"value":10},
            "color": {"value":"black"}
          }
        },
        {
          "data": {"url": "https://raw.githubusercontent.com/jdan0003/lab19/main/two-stars-michelin-restaurants.csv"},
          "mark": {"type": "circle", "tooltip": {"content":"data"}},
          "encoding": {
            "longitude": {"field":"longitude", "type":"quantitative"},
            "latitude": {"field":"latitude","type":"quantitative"},
            "size": {"value":10},
            "color": {"value":"yellow"}
          }
        },
        {
          "data": {"url": "https://raw.githubusercontent.com/jdan0003/lab19/main/three-stars-michelin-restaurants.csv"},
          "mark": {"type": "circle", "tooltip": {"content":"data"}},
          "encoding": {
            "longitude": {"field":"longitude", "type":"quantitative"},
            "latitude": {"field":"latitude","type":"quantitative"},
            "size": {"value":10},
            "color": {"value":"pink"}
          }
        }
    ]
  }